{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:dnd": "cypress run --spec \"cypress/e2e/*dnd*.cy.ts\"",
    "test:e2e:dnd:open": "cypress open --e2e --spec \"cypress/e2e/*dnd*.cy.ts\"",
    "test:ai-builder": "node scripts/setup-ai-builder-test.js",
    "quality": "npm run type-check && npm run lint && npm run test:coverage",
    "pre-commit": "npm run format && npm run quality",
    "dev:mock": "cross-env VITE_MOCK_AI=true VITE_ENABLE_AI_MOCK_MODE=true vite",
    "dev:ai-builder": "cross-env VITE_MOCK_AI=true VITE_ENABLE_AI_MOCK_MODE=true VITE_LOG_LEVEL=debug vite --mode development",
    "clean": "rimraf dist",
    "reset": "npm run clean && rimraf node_modules && npm install",
    "analyze": "npm run build && npx vite-bundle-analyzer",
    "deps": "npx depcheck",
    "outdated": "npm outdated"
  },
  "dependencies": {
    "@babel/generator": "^7.27.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@headlessui/react": "^2.2.3",
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^7.1.1",
    "@mui/material": "^7.1.1",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react": "^9.18.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@tinymce/miniature": "^6.0.0",
    "@tinymce/tinymce-react": "^6.2.0",
    "axios": "^1.9.0",
    "camelcase-keys": "^9.1.3",
    "chalk": "^5.3.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.5",
    "file-saver": "^2.0.5",
    "formik": "^2.4.6",
    "glob": "^11.0.2",
    "highlight.js": "^11.11.1",
    "html2canvas": "^1.4.1",
    "immer": "^10.0.4",
    "jwt-decode": "^4.0.0",
    "mkdirp": "^0.5.5",
    "pako": "^2.1.0",
    "prop-types": "^15.8.1",
    "quill": "^2.0.3",
    "react": "18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dnd-touch-backend": "^16.0.1",
    "react-docgen": "^8.0.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^6.0.0",
    "react-helmet-async": "^2.0.5",
    "react-icons": "^5.5.0",
    "react-player": "^2.16.0",
    "react-quill-new": "^3.4.6",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.5.2",
    "react-toastify": "^11.0.5",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-virtuoso": "^4.13.0",
    "react-window": "^1.8.11",
    "snakecase-keys": "^8.0.1",
    "tinymce": "^6.8.6",
    "yup": "^1.6.1",
    "zod": "^3.22.4",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.27.1",
    "@babel/traverse": "^7.27.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.0",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "depcheck": "^1.4.7",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.0.0",
    "husky": "^9.0.11",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.2",
    "msw": "^2.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.4.3",
    "vite": "^6.3.1",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^3.1.4"
  },
  "overrides": {
    "react-is": "18.2.0"
  }
}
